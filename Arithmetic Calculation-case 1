import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int x = obj.nextInt();
        int y = obj.nextInt();
        char z = obj.next().charAt(0);
        switch(z){
            case '+':
                float m = x+y;
                System.out.println("Addition of two number is "+m);
                break;
            case '-':
                float n = x-y;
                System.out.println("Subtraction of two number is "+n);
                break;
            case '*':
                float o = x*y;
                System.out.println("Multiplication of two number is "+o);
                break;
            case '/':
                float p = x/y;
                System.out.println("Division of two number is "+p);
                break;
            case '%':
                float q = x%y;
                System.out.println("Modulo of two number is "+q);
                break;
            default:
                System.out.println( "Invalid Input");
        }
               
    }
}
