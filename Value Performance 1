import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
        int a = obj.nextInt();
        float b = obj.nextFloat();
        System.out.println(a);
        System.out.printf("%.2f",b);
    }
}
