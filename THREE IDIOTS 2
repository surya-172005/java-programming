import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner s = new Scanner(System.in);
        int a1 = s.nextInt();
        int b1 = s.nextInt();
        int a2 = s.nextInt();
        int b2 = s.nextInt();
        double midA = (a1 + a2) /2;
        double midB = (b1 + b2) /2;
        System.out.printf("Binoy's house is located at (%.1f,%.1f)",midA,midB);
    }
}
