import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner obj = new Scanner(System.in);
       int x = obj.nextInt();
       if(x>0){
           System.out.printf("positive");
       } else if(x<0){
           System.out.printf("negative");
       } else{
           System.out.printf("zero");
       }
    }
}
