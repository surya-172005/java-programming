import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner obj = new Scanner(System.in);
        int a = obj.nextInt();
        System.out.printf("Bird said:%d",a);
        System.out.printf("\nAlice must go in path-%d",(a%10)+(a/10));
    }
}
